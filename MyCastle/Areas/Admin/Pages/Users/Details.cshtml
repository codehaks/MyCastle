@page
@model MyCastle.Areas.Admin.Pages.Users.DetailsModel
@{
}

<h1>Claims</h1>
<hr />

<p>@Model.SelectedUser.UserName</p>
<br />

<table>
    @foreach (var claim in Model.Claims)
    {
    <tr>
        <td>@claim.Type</td>

        <td>@claim.Value</td>
        <td>@claim.Issuer</td>
    </tr>
    }

</table>